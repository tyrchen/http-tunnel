# Environment-specific configuration
# Copy this file to .env and fill in your values

# Custom domain for HTTP API (e.g., tunnel.example.com)
TUNNEL_DOMAIN_NAME=tunnel.example.com

# Custom domain for WebSocket API (e.g., ws.example.com)
TUNNEL_WEBSOCKET_DOMAIN_NAME=ws.example.com

# ACM Certificate ARN covering both domains
TUNNEL_CERTIFICATE_ARN=arn:aws:acm:us-east-1:123456789012:certificate/your-cert-id-here
